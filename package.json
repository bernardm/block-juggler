{
	"name": "block-juggler",
	"displayName": "Block Juggler",
	"description": "Text extraction, transform, categorization, and shell processing.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:block-juggler.classify"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "block-juggler.classify",
				"title": "Parse tagged blocks"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"sanitize-filename": "^1.6.1"
	}
}
